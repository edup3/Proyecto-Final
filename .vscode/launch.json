{
    "configurations": [
        
    {
        "name": "(gdb) Debug gsea",
        "type": "cppdbg",
        "request": "launch",
        "program": "${workspaceFolder}/gsea",
        "args": ["-c", "-e", "--comp-alg", "lz77", "--enc-alg", "xor", "-i", "Texto.txt", "-o", "salida", "-k", "clave123"],
        "stopAtEntry": false,
        "cwd": "${fileDirname}",
        "environment": [],
        "externalConsole": false,
        "MIMode": "gdb",
        "setupCommands": [
            {
                "description": "Enable pretty-printing for gdb",
                "text": "-enable-pretty-printing",
                "ignoreFailures": true
            },
            {
                "description": "Set Disassembly Flavor to Intel",
                "text": "-gdb-set disassembly-flavor intel",
                "ignoreFailures": true
            }
        ]
    }
    ]
}